@charset "UTF-8";

@font-face {
  font-family: "crossover";
  src:url("../fonts/crossover.eot");
  src:url("../fonts/crossover.eot?#iefix") format("embedded-opentype"),
  url("../fonts/crossover.woff") format("woff"),
  url("../fonts/crossover.ttf") format("truetype"),
  url("../fonts/crossover.svg#crossover") format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "crossover" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "crossover" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-build:before {
  content: "\61";
}
.icon-down:before {
  content: "\63";
}
.icon-up:before {
  content: "\64";
}
.icon-left:before {
  content: "\65";
}
.icon-right:before {
  content: "\66";
}
.icon-search:before {
  content: "\67";
}
.icon-changeset:before {
  content: "\68";
}
