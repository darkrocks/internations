@charset "UTF-8";

@font-face {
  font-family: "internations";
  src:url("../fonts/internations.eot");
  src:url("../fonts/internations.eot?#iefix") format("embedded-opentype"),
    url("../fonts/internations.woff") format("woff"),
    url("../fonts/internations.ttf") format("truetype"),
    url("../fonts/internations.svg#internations") format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "internations" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "internations" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-search:before {
  content: "\61";
}
.icon-trash-o:before {
  content: "\62";
}
.icon-trash:before {
  content: "\63";
}
.icon-plus:before {
  content: "\64";
}
.icon-plus-circle:before {
  content: "\65";
}
